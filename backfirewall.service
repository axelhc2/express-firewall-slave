[Unit]
Description=Backfirewall Service
After=network.target

[Service]
Type=simple
User=root
WorkingDirectory=/root/backfirewall
ExecStart=/usr/bin/node /root/backfirewall/dist/index.js
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target


